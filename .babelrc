{
  "presets": [
    [
      "env",
      {
        "targets": {
          "node": 6
        }
      }
    ],
    "stage-1",
    "stage-2",
    "react"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "transform-runtime"
  ],
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ]
    },
    "production": {
      "comments": false,
      "presets": [
        "react-optimize"
      ],
      "plugins": [
        "lodash",
        "transform-remove-console"
      ]
    }
  }
}
